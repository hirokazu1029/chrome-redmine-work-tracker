{
  "manifest_version": 2,
  "name": "Redmine Work Tracker",
  "description": "Redmineで簡単に精度の高い作業時間の記録ができるようになります",
  "version": "0.2.6",
  "homepage_url": "https://github.com/negokaz/chrome-redmine-work-tracker/blob/master/manual.md",
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "icon/Stopclock_16.png",
    "32": "icon/Stopclock_32.png",
    "48": "icon/Stopclock_48.png",
    "128": "icon/Stopclock_128.png"
  },
  "background": {
    "scripts": [
      "bower_components/jquery/dist/jquery.js",
      "js/storage.js",
      "js/task.js",
      "js/timer.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "view/popup.html"
  },
  "options_page": "view/options.html"
}